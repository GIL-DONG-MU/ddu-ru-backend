# 운영 환경 설정
# ⚠️ 실제 운영 값으로 반드시 수정하세요!

# MySQL (운영용)
MYSQL_ROOT_USERNAME=root
MYSQL_ROOT_PASSWORD=
HOST_DB_PORT=3306
MYSQL_HOST=

# OAuth - Kakao (운영용)
KAKAO_CLIENT_ID=
KAKAO_CLIENT_SECRET=
KAKAO_REDIRECT_URI=https://your-domain.com/callback/kakao

# OAuth - Google (운영용)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=https://your-domain.com/callback/google

# JWT Configuration (운영용)
JWT_SECRET=
JWT_EXPIRATION=3600000

# Docker Image Tag
APP_TAG=latest

# Server IP (운영용)
SERVER_IP=
GHCR_PAT=